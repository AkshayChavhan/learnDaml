module Main where

import Daml.Script

-- first function
increment: Int -> Int
increment x = x + 1


-- second function
add : Int -> Int -> Int 
add x y = x + y


testFunctions: Script ()
testFunctions = script do
-- dollar is use to print result ,it is replacement of ()
  debug $ increment 3
-- here is the example where () replaced with $
  debug (increment 4)
  -- below is for second function add
  debug $ add 3 3
  debug (add 3 5)